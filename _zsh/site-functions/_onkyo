_arguments -s -S \
  '--host[Connect to this host]:' \
  '--port[Connect to this port]:' \
  '--all[Discover receivers, send to all found]:' \
  '--name[Discover receivers, send to those matching name]:' \
  '--discover[List all discoverable receivers]:' \
  '--help-commands[List available commands]:' \
  '*:: :->subcmds' && return 0

if (( CURRENT == 1 )); then
  _describe -t commands 'onkyo command' _commands
  return
fi

